{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "intelephense.telemetry.enabled": false,
    "terminal.integrated.enablePersistentSessions": false,
    "zenMode.fullScreen": false,
    "zenMode.silentNotifications": false,
    "editor.fontSize": 13,
    "editor.find.loop": false,
    "editor.minimap.enabled": false,
    "window.restoreWindows": "none",
    "window.menuBarVisibility": "toggle",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-D HH:mm",
    "gitlens.defaultDateShortFormat": "YYYY-MM-D",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.currentLine.enabled": false,
    "gitlens.views.commits.files.layout": "tree",
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "autoHide.sideBarDelay": 0,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "workbench.preferredLightColorTheme": "Quiet Light",
    "workbench.preferredDarkColorTheme": "Base16 Tomorrow",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.editor.limit.enabled": true,
    "workbench.colorCustomizations": {
        "terminal.foreground": "#e2dddd",
        "terminal.background": "#333333"
    }
}
