{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "intelephense.telemetry.enabled": false,
    "terminal.integrated.defaultProfile.linux": "tmux",
    "terminal.integrated.enablePersistentSessions": false,
    "zenMode.fullScreen": false,
    "zenMode.silentNotifications": false,
    "zenMode.hideLineNumbers": false,
    "editor.fontSize": 13,
    "editor.find.loop": false,
    "editor.minimap.enabled": false,
    "editor.unfoldOnClickAfterEndOfLine": true,
    "window.restoreWindows": "none",
    "window.menuBarVisibility": "toggle",
    "search.mode": "reuseEditor",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-D HH:mm",
    "gitlens.defaultDateShortFormat": "YYYY-MM-D",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.currentLine.enabled": false,
    "gitlens.views.commits.files.layout": "tree",
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "autoHide.sideBarDelay": 0,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/init.vscode_neovim.vim",
    "workbench.preferredLightColorTheme": "Quiet Light",
    "workbench.preferredDarkColorTheme": "Base16 Tomorrow",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.editor.limit.enabled": true,
    "workbench.sideBar.location": "right",
    "workbench.colorCustomizations": {
        "terminal.foreground": "#e2dddd",
        "terminal.background": "#333333"
    },
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "Quiet Light",
}
