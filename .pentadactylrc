" Mutassa a konyvjelzosavot es a toolbar-t.
set guioptions+=BT

" Mindig mutassa a parancssort.
set guioptions+=c

" Mutassa a tabok sorszamat.
set guioptions+=n

" Kenyelmesebben elerheto billentyuk a navigalashoz.
set hintkeys=asdfghjklqwertzuiop

" Jobban lathato karakterek a navigalashoz.
highlight Hint font-size:16px;color:white;background-color:red;padding:2px;

" Map.
map  -builtin  <C-J>  <CR>
imap -builtin  <C-J>  <CR>
cmap -builtin  <C-J>  <CR>
map  -builtin  <C-C>  <Esc>
imap -builtin  <C-C>  <Esc>
cmap -builtin  <C-C>  <Esc>
cmap -builtin  <C-N>  <Tab>
cmap -builtin  <C-P>  <S-Tab>
map  -builtin  s      f
map  -builtin  <C-H>  d
imap -builtin  <C-L>  <Del>
nmap -builtin  <C-L>  :reload!<CR>
nmap -builtin  <F12>  :nohlsearch<CR>

" Scroll gyorsitasa.
nmap -builtin  j      3j
nmap -builtin  k      3k

" Command.
command  -nargs=+ -complete url  e     open <args>
command  -nargs=+ -complete url  tabe  tabopen <args>

" vim: filetype=vim
